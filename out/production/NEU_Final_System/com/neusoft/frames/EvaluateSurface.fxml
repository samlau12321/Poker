<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane stylesheets="@../Styles/EvaluateSurfaceStyle.css" prefHeight="726.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.neusoft.controller.EvaluateSurfaceController">
    <children>
      <ImageView fitHeight="724.0" fitWidth="1200.0">
         <image>
            <Image url="@../../../picture/Welcome1.jpg" />
         </image>
      </ImageView>
        <TableView fx:id="InfoTable" layoutX="101.0" layoutY="75.0" prefHeight="483.0" prefWidth="999.0" AnchorPane.leftAnchor="101.0" AnchorPane.rightAnchor="100.0">
            <columns>
                <TableColumn fx:id="nameColumn" text="姓名" />
                <TableColumn fx:id="sexColumn" minWidth="1.0" prefWidth="159.0" text="性别" />
                <TableColumn fx:id="templateNameColumn" prefWidth="98.0" text="模板ID" />
