            <TableColumn fx:id="evaluatorColumn" prefWidth="134.0" text="评估人" />
            <TableColumn fx:id="judgementColumn" prefWidth="100.0" text="评估意见" />
            <TableColumn fx:id="scoreColumn" prefWidth="75.0" text="评估得分" />
