        <Button fx:id="exitButton" layoutX="955.0" layoutY="587.0" mnemonicParsing="false" onAction="#click_back" prefHeight="58.0" prefWidth="145.0" text="返回">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
