<AnchorPane stylesheets="@../Styles/ViewQuestionStyle.css" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.neusoft.controller.QuestionViewController">
