                <TableColumn fx:id="ChoiceAColumn" prefWidth="75.0" text="选项 A" />
                <TableColumn fx:id="ChoiceBColumn" prefWidth="75.0" text="选项 B" />
                <TableColumn fx:id="ChoiceCColumn" prefWidth="75.0" text="选项 C" />
            </columns>
        </TableView>
          <Button fx:id="exitButton" layoutX="878.0" layoutY="594.0" mnemonicParsing="false" onAction="#click_exit" prefHeight="63.0" prefWidth="170.0" text="返回">
