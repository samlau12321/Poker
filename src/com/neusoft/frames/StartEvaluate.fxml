<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane stylesheets="@../Styles/StartEvaluateStyle.css" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.neusoft.controller.StartEvaluateController">
    <children>
      <ImageView fitHeight="800.0" fitWidth="1200.0">
         <image>
            <Image url="@../../../picture/Welcome1.jpg" />
         </image>
      </ImageView>
        <TableView fx:id="InfoTable" layoutX="100.0" layoutY="160.0" prefHeight="444.0" prefWidth="1000.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
            <columns>
                <TableColumn fx:id="selectColumn" prefWidth="250.0" text="选择框" />
                <TableColumn fx:id="IDColumn" minWidth="1.0" prefWidth="250.0" text="模板ID" />
                <TableColumn fx:id="typeColumn" prefWidth="250.0" text="模板类型" />
            </columns>
        </TableView>
      <HBox layoutX="100.0" layoutY="146.0" spacing="100.0" />
      <Button fx:id="exitButton" layoutX="966.0" layoutY="635.0" mnemonicParsing="false" onAction="#click_back" prefHeight="57.0" prefWidth="134.0" text="返回">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="100.0" layoutY="635.0" mnemonicParsing="false" onAction="#click_startEvaluate" prefHeight="57.0" prefWidth="161.0" text="开始评估" />
    </children>
</AnchorPane>
